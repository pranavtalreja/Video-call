  "bugs": {
    "url": "https://github.com/info-arnav/videoCall/issues"
  },
  "homepage": "https://github.com/info-arnav/videoCall#readme",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-pino-logger": "^5.0.0",
    "jquery": "^3.5.1",
    "twilio": "^3.48.1"
  }
}
