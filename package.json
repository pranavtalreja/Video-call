{
  "name": "videoCall",
  "version": "1.0.0",
  "description": "##### this web application is designed on node and react and functions on a single github page",
  "main": "index.js",
  "scripts": {
    "fronted-windows": "cd app/ && npm start-windows",
    "fronted-linuxmac": "cd app/ && npm run start-linuxmac",
    "servif": "cd applicationServer/ && node server.ts",
    "vserif": "cd video/server/ && node index.js",
    "vfronted": "cd video/ && npm run start",
    "tsinge-windows": "concurrently \"npm run servif\" \"npm run fronted-windows\" \"npm run vserif\" \"npm run vfronted\"",
    "tsinge-linuxmac": "concurrently \"npm run servif\" \"npm run fronted-linuxmac\" \"npm run vserif\" \"npm run vfronted\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/info-arnav/videoCall.git"
  },
